{
    "name": "Light 新标签页",
    "description": "轻量简洁的新标签页",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "bookmarks"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_icon": "images/icons/logo128.png",
        "default_title": "Light 新标签页"
    },
    "icons": {
        "32": "images/icons/logo32.png",
        "128": "images/icons/logo128.png"
    }
}